{
  "version": 2,
  "name": "real-time-collaborative-code-editor",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)\\.(js|css|ico|png|jpg|jpeg|gif|svg)",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_BACKEND_URL": "@backend_url"
  },
  "build": {
    "env": {
      "REACT_APP_BACKEND_URL": "@backend_url"
    }
  }
}
